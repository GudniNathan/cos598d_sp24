{
    "emeraldwalk.runonsave": {
        "commands": [
            {
                "match": ".*",
                "isAsync": false,
                "cmd": "git add ."
            },
            {
                "match": ".*",
                "isAsync": false,
                "cmd": "git commit -a -m \"vscode autosave\"",
            },
        ]
    }
}